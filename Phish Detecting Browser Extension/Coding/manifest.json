{
    "manifest_version": 2,
    "name": "PhishDetect",
    "version": "1.0",
    "description": "Real-time phishing detection browser extension.",
    "permissions": [
      "activeTab",
      "webRequest",
      "webRequestBlocking",
      "<all_urls>"
    ],
    "background": {
      "scripts": ["background.js"]
    },
    "browser_action": {
      "default_popup": "popup.html",
      "default_icon": {
        "48": "icons/phishdetect.png"
      }
    }
  }
  